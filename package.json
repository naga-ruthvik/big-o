{
  "name": "big-o",
  "private": true,
  "version": "0.0.0",
  "description": "BigO DSA Tracker Desktop App",
  "author": "Ruthvik",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"vite\" \"wait-on tcp:3000 && electron .\"",
    "electron:build": "vite build -c vite.electron.config.ts && electron-builder"
  },
  "dependencies": {
    "@google/genai": "^1.36.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "recharts": "^3.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.bigo.tracker",
    "productName": "BigO",
    "directories": {
      "output": "dist-app"
    },
    "files": [
      "dist-electron/**/*",
      "electron/**/*",
      "public/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "signAndEditExecutable": false
    }
  }
}